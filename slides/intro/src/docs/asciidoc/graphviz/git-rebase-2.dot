digraph git_merge_fast_fwd {
    splines=line
    rankdir=LR
    bgcolor=transparent

    node[width=0.3, shape=rect,fontsize=12.0, style="", color=orange]
    edge[dir=back, style=""]
    master [label="master"]
    feature [label="feature"]

    node[shape=circle,fontsize=12.0, style="", color=""]
    edge[dir=back, style=""]

    c_0 [label="C 0", group=master]
    c_1 [label="C 1", group=master]
    c_2 [label="C 2", group=master]
    c_3 [label="C 3", group=master]

    /* Marked nodes */
    node[shape=circle, color=blue, style=bold]
    c_4 [label="C 4 '", group=master]
    node[style=dashed]
    c_4_ [label="C 4", group=branch]

    edge[style=""]
    c_0 -> c_1 -> c_2
    c_2 -> c_3
    c_2 -> c_4_
    master -> c_3 [dir=""]
    feature -> c_4 [dir=""]
    c_3 -> c_4
    edge[color=green]
    master -> c_4[label="Fast Fwd.", dir=""]

    {rank=same; c_3 c_4_ master}
    {rank=same; c_4 feature}
}
